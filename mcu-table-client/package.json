    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "buble": "^0.15.2",
    "buble-loader": "^0.4.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.26.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "html-minifier": "^3.5.7",
